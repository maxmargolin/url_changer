{
  "manifest_version": 2,

  "name": "URL Redirector",
  "description": "Redirect url by pattern to any place you want.",
  "version": "1.0",
  "background":{
    "scripts":["bg.js"]
  },
  "permissions": [
    "webRequest",
    "*://*/*",
    "storage",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_title": "URL Redirector",
    "default_popup": "index.html"
  }
}